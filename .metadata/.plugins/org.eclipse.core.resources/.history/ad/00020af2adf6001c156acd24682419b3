var name='Global Haspital';
phone=54564654654654;

function show(){
	
	console.log('Name '+name);
	console.log('Phone Number '+phone);
	
	var localBranch='velacherry';
	console.log('Branch '+localBranch);
}

function invoke(){
	
	show();
	
	console.log('Name '+name);
