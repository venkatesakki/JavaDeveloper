/**
 * Example on Different ways of writing javaScript function
 */


//simple named function

function simple()
{
	console.log("Hello World");
}

function withArgs(name)
{
	if(name==null)
	{
		document.write(' welcome Guest ')
		
	}else{
			document.write(' welcome '+name);
	}

}


(function(){
	
	simple();
	withArgs();
	withArgs('venkat');
	
})();